apache_vhosts:
  - servername: "{{ inventory_hostname }}"
    extra_parameters: |
        RewriteEngine on
        RewriteRule ^(.*) http://localhost:8080$1 [P]

apache_mods_enabled:
  - rewrite.load
  - proxy.load
  - proxy_http.load
  - proxy.conf

object_store_directory: /srv/galaxy/database
galaxy_version: dev
force: yes
apache_remove_default_vhost: True
